{
  "name": "@portfolio-chat/prisma-schema",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "scripts": {
    "generate": "prisma generate",
    "migrate:dev": "prisma migrate dev --schema=./prisma/schema.prisma",
    "migrate:deploy": "dotenv -e ./.env.production -- prisma migrate deploy --schema=./prisma/schema.prisma"
  },
  "devDependencies": {
    "@prisma/client": "^6.12.0",
    "dotenv-cli": "^10.0.0",
    "prisma": "^6.12.0"
  }
}
